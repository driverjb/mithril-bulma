{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "checkJs": false,
    "noEmit": false,
    "outDir": "types",
    "lib": ["ESNext", "DOM"],
    "removeComments": false,
    "moduleResolution": "bundler",
    "module": "esnext"
  },
  "include": [
    "columns/**/*.mjs",
    "components/**/*.mjs",
    "elements/**/*.mjs",
    "form/**/*.mjs",
    "grid/**/*.mjs",
    "layout/**/*.mjs",
    "attributes.mjs",
    "util.mjs",
    "index.mjs"
  ]
}
